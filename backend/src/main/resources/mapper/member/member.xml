<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.me.backend.member.mapper.MemberMapper">

    <select id="getMemberList" resultType="com.me.backend.member.vo.MemberVO">
        SELECT * FROM foryou.customer;
    </select>

    <select id="member" resultType="com.me.backend.member.vo.MemberVO">
        select
            cust_id,
            cust_pwd,
            cust_name,
            cust_email,
            cust_tel,
            mileage,
            status
        from foryou.customer
        where cust_id = #{cust_id} and cust_pwd = #{cust_pwd} and status = 'N'
    </select>

</mapper>